{
  "name": "gocabs",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start --reset-cache",
    "test": "jest",
    "clean": "npm cache clean --force",
    "build:android": "cd android && ./gradlew clean && ./gradlew assembleRelease",
    "build:android-debug": "cd android && ./gradlew clean && ./gradlew assembleDebug",
    "setLocal": "ENVFILE=.env.development",
    "setProduction": "ENVFILE=.env.production",
    "android-local": "npm run setLocal && npm run build:android",
    "android-production": "npm run setProduction && npm run build:android",
    "android-debug": "npm run setLocal && npm run build:android-debug",
    "build:android:release": "cd android && ENVFILE=.env.production ./gradlew clean && ./gradlew assembleRelease",
    "build:android:debug": "cd android && ENVFILE=.env.development ./gradlew clean && ./gradlew assembleDebug",
    "install:release": "adb install -r android/app/build/outputs/apk/release/app-release.apk",
    "install:debug": "adb install -r android/app/build/outputs/apk/debug/app-debug.apk",
    "test:release": "npm run build:android:release && npm run install:release",
    "broadcast-server": "npx ts-node -r tsconfig-paths/register src/services/socket/broadcastServer.ts"
  },
